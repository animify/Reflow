    "history": "^4.7.2",
    "immer": "^1.3.1",
    "jeet": "^7.2.0",
    "nib": "^1.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.1.1",
    "react-draggable": "^3.0.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-hotkeys": "^1.1.4",
    "react-redux": "^4.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-svg-pan-zoom": "^2.17.0",
