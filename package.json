{
  "name": "webpack-react-express",
  "version": "1.0.0",
  "description": "A simple server and client boilerplate built on React, Express, Webpack and styled by Blossom.",
  "main": "server/server.js",
  "scripts": {
    "start:server": "rimraf ./dist && webpack --config  webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "rimraf ./dist && npm-run-all --parallel start:server start:client",
    "build": "webpack --config  webpack.prod.client.js",
    "eslint": "eslint ."
  },
  "author": "Stefan Mansson <st.mansson@icloud.com> (http://mansson.io)",
  "license": "MIT",
